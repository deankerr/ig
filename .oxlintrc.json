{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["eslint", "typescript", "unicorn", "oxc", "react", "react-perf", "promise", "import"],
  "overrides": [
    {
      "files": ["packages/env/**"],
      "rules": {
        "typescript/triple-slash-reference": "off"
      }
    }
  ],
  "ignorePatterns": ["**/routeTree.gen.ts", "**/.alchemy/**"]
}
